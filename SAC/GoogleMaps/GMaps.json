{
    "id": "com.hasba.sac.gmaps",
    "version": "0.0.2",
    "name": "Google Maps",
    "description": "Google Maps demo",
    "newInstancePrefix": "gmap",
    "icon": "https://fhasba1204.github.io/SAC/GoogleMaps/888856.png",
    "vendor": "Hasba",
    "eula": "",
    "license": "",
    "imports": [
        "input-controls"
    ],
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-hasba-sac-gmaps",
            "url": "https://fhasba1204.github.io/SAC/GoogleMaps/GMP.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
			"kind": "builder",
			"tag": "com-hasba-sac-gmaps-bps",
			"url": "https://fhasba1204.github.io/SAC/GoogleMaps/gmaps_bps.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
    ],
    "properties": {
        "metadata": {
            "type": "string",
            "description": "For internal use",
            "default": ""
        },
        "geochar": {
			"type": "string",
			"description": "Geo Characteristic",
			"default": ""
		},
        "latitude": {
			"type": "string",
			"description": "Latitude Characteristic",
			"default": ""
		},
        "longitude": {
			"type": "string",
			"description": "Longitude Characteristic",
			"default": ""
		},
        "content": {
			"type": "string",
			"description": "Content Characteristic",
			"default": ""
		},
        "selection": {
			"type": "string",
			"description": "Data Selection",
			"default": ""
		},
        "width": {
            "type": "integer",
            "description": "Width",
            "default": 700
        },
        "height": {
            "type": "integer",
            "description": "Height",
            "default": 660
        }
    },
    "methods": {
        "render": {
            "description": "Render",
            "parameters": [
                {
                    "name": "resultSet",
                    "type": "any",
                    "description": "The json"
                }
            ]
        },
        "clearMapMarkers": {
            "description": "Clear Map Markers",
                "parameters": []
        },
        "clearCircle": {
            "description": "Clear Map Circle",
                "parameters": []
        },
        "drawCircle":{
            "description": "Draw Map Circle",
                "parameters": [{
					"name": "latitude",
					"type": "number",
					"description": "Latitude"
				},
				{
					"name": "longitude",
					"type": "number",
					"description": "Longitude"
				},
				{
					"name": "radius",
					"type": "number",
					"description": "Radius in KM"
				}]
        },
        "setSelection": {
            "parameters": [
                {"name": "newSelection", "type": "string"}
            ]
            },
        "getSelection": {
			"returnType": "string",
			"description": "Returns the selected content of the marker."
		}
        
    },
    "events": {
        "onClick": {
			"description": "Called when the user clicks the Markera."
		}
    }
}